generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model User {
  id          Int       @id @default(autoincrement())
  name        String
  email       String    @unique
  password    String
  number      String?
  dateOfBirth DateTime?
  avatar      String?
  emotions    Emotion[]
}

model Emotion {
  id        Int      @id @default(autoincrement())
  label     String
  user      User    @relation(fields: [userId], references: [id])
  userId    Int
  createdAt DateTime @default(now())
}
